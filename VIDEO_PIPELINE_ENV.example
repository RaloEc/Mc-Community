# Configuración para Pipeline de Video Asíncrono
# Copiar este archivo a .env.local y llenar los valores

# Supabase (Ya debería estar configurado)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Configuración de Video (Opcional, para límites personalizados)
NEXT_PUBLIC_VIDEO_MAX_SIZE_MB=500
NEXT_PUBLIC_VIDEO_ALLOWED_TYPES=video/mp4,video/webm,video/quicktime,video/x-msvideo

# Configuración de Edge Function (Opcional)
NEXT_PUBLIC_VIDEO_CONVERTER_TIMEOUT=600000  # 10 minutos en ms

# Configuración de Storage
NEXT_PUBLIC_VIDEO_UPLOADS_BUCKET=video-uploads
NEXT_PUBLIC_VIDEOS_BUCKET=videos

# Configuración de Realtime (Opcional)
NEXT_PUBLIC_REALTIME_ENABLED=true
NEXT_PUBLIC_REALTIME_TIMEOUT=30000  # 30 segundos en ms
